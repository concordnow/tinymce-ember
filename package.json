{
  "name": "tinymce-ember",
  "version": "0.0.0",
  "description": "TinyMCE Ember addon",
  "keywords": [
    "ember-addon",
    "tinymce",
    "editor"
  ],
  "repository": "https://github.com/concordnow/tinymce-ember",
  "license": "MIT",
  "author": "Concord Worlwide Inc.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "npm-run-all --aggregate-output --continue-on-error --parallel build:*",
    "build:ember": "ember build --environment=production",
    "build:storybook": "build-storybook -s dist",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "npm-run-all --aggregate-output --continue-on-error --parallel start:*",
    "start:ember": "ember serve",
    "start:storybook": "start-storybook -p 6006 -s dist",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-essentials": "^6.1.9",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/ember": "^6.1.9",
    "@storybook/ember-cli-storybook": "^0.3.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-ember-modules-api-polyfill": "^3.2.0",
    "babel-plugin-htmlbars-inline-precompile": "^4.2.1",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-funnel": "^3.0.3",
    "core-js": "^3.8.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.22.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.22.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.11.0",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.5.0",
    "tinymce": "^5.5.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
